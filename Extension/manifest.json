{
  "manifest_version": 2,
  "name": "human_app_label",
  "version": "1.0",

  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "drmcgarry@networking",
      "strict_min_version": "50.0"
    }
  },
  "permissions": [
    "contextMenus",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "tabs"
  ]
}
